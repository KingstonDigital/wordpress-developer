		"labelMessageTypography": {
			"type": "object",
			"default": {}
		},
		"inputFirstNameTypography": {
			"type": "object",
			"default": {}
		},
		"inputLastNameTypography": {
			"type": "object",
			"default": {}
		},
		"inputEmailTypography": {
			"type": "object",
			"default": {}
		},
		"inputPhoneTypography": {
			"type": "object",
			"default": {}
		},
		"inputMessageTypography": {
			"type": "object",
			"default": {}
		},
		"buttonTypography": {
			"type": "object",
			"default": {}
		}
